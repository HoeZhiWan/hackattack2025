{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "security_smile",
  "version": "0.1.0",
  "identifier": "com.lengzailenglui.sm",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:3000",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../out"
  },  "app": {    
    "windows": [
      {
        "title": "Security Smile",
        "width": 800,
        "height": 600,
        "visible": true,
        "center": true,
        "skipTaskbar": false
      }
    ],
    "security": {
      "csp": null
    },
    "withGlobalTauri": true
  },
    "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/favicon.ico"
    ]
  }
}
