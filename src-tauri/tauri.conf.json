{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "hackattack",
  "version": "0.1.0",
  "identifier": "com.malaysia.abang",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:3000",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../out"
  },  "app": {    
    "windows": [
      {
        "title": "hackattack",
        "width": 800,
        "height": 600,
        "visible": true,
        "center": true,
        "skipTaskbar": false
      }
    ],
    "security": {
      "csp": null
    },
    "withGlobalTauri": true
  },
  
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
